{
	"compilerOptions": {
		"target": "es2015",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"baseUrl": ".",
		"paths": {
			"#/*": ["./*"],
			"#/public/*": ["./public/*"],
			"#/app/*": ["./src/app/*"],
			"#/components/*": ["./src/components/*"],
			"#/constants/*": ["./src/constants/*"],
			"#/providers/*": ["./src/providers/*"],
			"#/hooks/*": ["./src/hooks/*"],
			"#/services/*": ["./src/services/*"],
			"#/providers/*": ["./src/providers/*"],
			"#/styles/*": ["./src/styles/*"],
			"#/types/*": ["./src/types/*"],
			"#/utils/*": ["./src/utils/*"],
			"#/views/*": ["./src/views/*"],
			"#/stores/*": ["./src/stores/*"],
			"#/libs/*": ["./src/libs/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
